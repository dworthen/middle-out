{
  "name": "middle-out",
  "version": "1.0.0-beta.10",
  "description": "",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/typings/index.d.ts",
  "scripts": {
    "test": "jest __tests__/.*\\.ts",
    "clean": "rimraf ./dist/*",
    "build:debugjest": "tsc -p tsconfig.debugjest.json",
    "build:commonjs": "tsc -p tsconfig.app.json",
    "build:esm": "tsc -p tsconfig.app.json --target ES2015 --module ES2015 --outDir ./dist/esm/ --declaration true --declarationDir ./dist/typings/",
    "build:umd": "webpack",
    "prebuild:umd": "npm run clean && npm run build:esm",
    "release": "standard-version",
    "prerelease2": "npm run coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "precoverage": "npm test -- --coverage",
    "prepublishOnly": "npm run build:umd"
  },
  "keywords": [],
  "files": [
    "dist/esm/**/*",
    "dist/typings/**/*",
    "dist/umd/**/*"
  ],
  "author": "Derek Worthen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/validator": "^9.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "coveralls": "^3.0.0",
    "jest": "22.4.1",
    "jest-junit": "^4.0.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.3.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.3",
    "typescript": "^2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "validator": "^10.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testResultsProcessor": "jest-junit",
    "collectCoverageFrom": [
      "**/src/**/*.{js,ts,jsx,tsx}",
      "!**/src/**/index.ts",
      "!dist/**/*"
    ],
    "coverageReporters": [
      "lcov",
      "json",
      "cobertura"
    ],
    "transform": {
      "\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "jest-junit": {
    "output": "./coverage/junit.xml"
  }
}
